{
  "surah_number": 87,
  "name_en": "Al-A'la",
  "name_ar": "الأعلى",
  "verses_count": 19,
  "revelation_place": "makkah",
  "verses": [
    {
      "global_id": 5949,
      "surah": 87,
      "ayah": 1,
      "verse_key": "87:1",
      "text_ar": " سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى",
      "text_en_pickthall": "Praise the name of thy Lord the Most High,"
    },
    {
      "global_id": 5950,
      "surah": 87,
      "ayah": 2,
      "verse_key": "87:2",
      "text_ar": "ٱلَّذِى خَلَقَ فَسَوَّىٰ",
      "text_en_pickthall": "Who createth, then disposeth;"
    },
    {
      "global_id": 5951,
      "surah": 87,
      "ayah": 3,
      "verse_key": "87:3",
      "text_ar": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ",
      "text_en_pickthall": "Who measureth, then guideth;"
    },
    {
      "global_id": 5952,
      "surah": 87,
      "ayah": 4,
      "verse_key": "87:4",
      "text_ar": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ",
      "text_en_pickthall": "Who bringeth forth the pasturage,"
    },
    {
      "global_id": 5953,
      "surah": 87,
      "ayah": 5,
      "verse_key": "87:5",
      "text_ar": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ",
      "text_en_pickthall": "Then turneth it to russet stubble."
    },
    {
      "global_id": 5954,
      "surah": 87,
      "ayah": 6,
      "verse_key": "87:6",
      "text_ar": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ",
      "text_en_pickthall": "We shall make thee read (O Muhammad) so that thou shalt not forget"
    },
    {
      "global_id": 5955,
      "surah": 87,
      "ayah": 7,
      "verse_key": "87:7",
      "text_ar": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ",
      "text_en_pickthall": "Save that which Allah willeth. Lo! He knoweth the disclosed and that which still is hidden;"
    },
    {
      "global_id": 5956,
      "surah": 87,
      "ayah": 8,
      "verse_key": "87:8",
      "text_ar": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
      "text_en_pickthall": "And We shall ease thy way unto the state of ease."
    },
    {
      "global_id": 5957,
      "surah": 87,
      "ayah": 9,
      "verse_key": "87:9",
      "text_ar": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ",
      "text_en_pickthall": "Therefor remind (men), for of use is the reminder."
    },
    {
      "global_id": 5958,
      "surah": 87,
      "ayah": 10,
      "verse_key": "87:10",
      "text_ar": "سَيَذَّكَّرُ مَن يَخْشَىٰ",
      "text_en_pickthall": "He will heed who feareth,"
    },
    {
      "global_id": 5959,
      "surah": 87,
      "ayah": 11,
      "verse_key": "87:11",
      "text_ar": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى",
      "text_en_pickthall": "But the most hapless will flout it,"
    },
    {
      "global_id": 5960,
      "surah": 87,
      "ayah": 12,
      "verse_key": "87:12",
      "text_ar": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ",
      "text_en_pickthall": "He who will be flung to the great Fire"
    },
    {
      "global_id": 5961,
      "surah": 87,
      "ayah": 13,
      "verse_key": "87:13",
      "text_ar": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
      "text_en_pickthall": "Wherein he will neither die nor live."
    },
    {
      "global_id": 5962,
      "surah": 87,
      "ayah": 14,
      "verse_key": "87:14",
      "text_ar": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
      "text_en_pickthall": "He is successful who groweth,"
    },
    {
      "global_id": 5963,
      "surah": 87,
      "ayah": 15,
      "verse_key": "87:15",
      "text_ar": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ",
      "text_en_pickthall": "And remembereth the name of his Lord, so prayeth,"
    },
    {
      "global_id": 5964,
      "surah": 87,
      "ayah": 16,
      "verse_key": "87:16",
      "text_ar": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
      "text_en_pickthall": "But ye prefer the life of the world"
    },
    {
      "global_id": 5965,
      "surah": 87,
      "ayah": 17,
      "verse_key": "87:17",
      "text_ar": "وَٱلْـَٔاخِرَةُ خَيْرٌ وَأَبْقَىٰٓ",
      "text_en_pickthall": "Although the Hereafter is better and more lasting."
    },
    {
      "global_id": 5966,
      "surah": 87,
      "ayah": 18,
      "verse_key": "87:18",
      "text_ar": "إِنَّ هَـٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ",
      "text_en_pickthall": "Lo! This is in the former scrolls."
    },
    {
      "global_id": 5967,
      "surah": 87,
      "ayah": 19,
      "verse_key": "87:19",
      "text_ar": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ",
      "text_en_pickthall": "The Books of Abraham and Moses."
    }
  ]
}
