    },
    {
      "global_id": 6117,
      "surah": 96,
      "ayah": 11,
      "verse_key": "96:11",
      "text_ar": "أَرَءَيْتَ إِن كَانَ عَلَى ٱلْهُدَىٰٓ",
      "text_en_pickthall": "Hast thou seen if he relieth on the guidance (of Allah)"
    },
    {
      "global_id": 6118,
      "surah": 96,
      "ayah": 12,
      "verse_key": "96:12",
      "text_ar": "أَوْ أَمَرَ بِٱلتَّقْوَىٰٓ",
